{
  "name": "manuel-manero-api",
  "version": "1.0.0",
  "description": "Backend API para Ecossistema UAU",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node --esm src/index.ts",
