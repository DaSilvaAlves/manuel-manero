    "typescript": "^5.3.3",
    "@types/node": "^20.10.0",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "ts-node": "^10.9.1",
    "zod": "^3.22.4",
    "axios": "^1.6.0",
    "redis": "^4.6.11",
    "@types/redis": "^4.0.11",
    "bull": "^4.11.4",
    "@types/bull": "^3.15.9",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "bcryptjs": "^2.4.3",
